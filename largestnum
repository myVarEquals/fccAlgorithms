function largestOfFour(arr) {
 var largestArr = [];
   for (i = 0; i < arr.length; i++) {
    var largest = arr[i][0];
    for (j = 1; j < arr[i].length; j++) {
      if (largest < arr[i][j]) {
       largest = arr[i][j];
      }
      
    }
    largestArr[i] = largest;
  }
    
  return largestArr;
}

largestOfFour([[13, 27, 18, 26], [4, 5, 1, 3], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
